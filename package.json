    "@types/node": "^22.15.21",
    "jest": "^30.0.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.2",
